local Library = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local Window = Library:CreateWindow({
    Title = "Roblox Scripts",
    SubTitle = "by Godlike",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Resize = true,
    MinSize = Vector2.new(350, 300),
    Acrylic = true,
    Theme = "light",
    MinimizeKey = Enum.KeyCode.RightControl
})

Library:Notify({
    Title = "Welcome",
    Content = "Enjoy my Script",
    SubContent = "Join the discord to suggest future updates",
    Duration = 3
})

local Tabs = {
    Players = Window:AddTab({ Title = "Players", Icon = "activity" }),
    Client = Window:AddTab({ Title = "Client", Icon = "bot" }),
    Teleports = Window:AddTab({ Title = "Teleports", Icon = "cctv" }),
    Music = Window:AddTab({ Title = "Music", Icon = "music" }),
    BugReports = Window:AddTab({ Title = "Bug Reports", Icon = "bug" }),
    OtherScripts = Window:AddTab({ Title = "Other Scripts", Icon = "file-archive" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "book-check" })
}

local Library = safeLoad("https://github.com/ActualMasterOogway/Fluent-Renewed/releases/latest/download/Fluent.luau", "Fluent Library")
local SaveManager = safeLoad("https://raw.githubusercontent.com/ActualMasterOogway/Fluent-Renewed/main/Addons/SaveManager.luau", "Save Manager")
local InterfaceManager = safeLoad("https://raw.githubusercontent.com/ActualMasterOogway/Fluent-Renewed/main/Addons/InterfaceManager.luau", "Interface Manager")

local Window = Library:CreateWindow({
    Title = "Roblox Scripts",
    SubTitle = "By Godlike",
    TabWidth = 125,
    Size = UDim2.fromOffset(830, 525),
    Resize = true,
    MinSize = Vector2.new(470, 380),
    Acrylic = true,
    Theme = "Viow Mars",
    MinimizeKey = Enum.KeyCode.RightControl
})

-- Fluent provides Lucide Icons, they are optional
local Tabs = {
    Players = Window:AddTab({ Title = "Players", Icon = "bot" }),
    Client = Window:AddTab({ Title = "Client", Icon = "box" }),
    Teleports = Window:AddTab({ Title = "Teleports", Icon = "cable" }),
    Music = Window:AddTab({ Title = "Music", Icon = "music" }),
    Bugs = Window:AddTab({ Title = "Bug Reports", Icon = "bug" }),
    Scripts = Window:AddTab({ Title = "Scripts", Icon = "file" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "check" })
}
